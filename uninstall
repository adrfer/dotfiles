#!/usr/bin/env bash

# Uninstall 'dotfiles-bash' from 'https://github.com/adrfer/dotfiles-bash.git'
echo "=> Uninstalling 'dotfiles-bash'..."
bash <(curl -sL https://raw.github.com/adrfer/dotfiles-bash/master/uninstall)

# Uninstall 'dotfiles-readline' from 'https://github.com/adrfer/dotfiles-readline.git'
echo "=> Uninstalling 'dotfiles-readline'..."
bash <(curl -sL https://raw.github.com/adrfer/dotfiles-readline/master/uninstall)